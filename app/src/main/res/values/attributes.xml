    <declare-styleable name="CovidTextView">
        <attr name="typeface" format="enum">
            <enum name="regular" value="0"/>
            <enum name="bold" value="1"/>
            <enum name="thin" value="2"/>
            <enum name="medium" value="3"/>
        </attr>
    </declare-styleable>
